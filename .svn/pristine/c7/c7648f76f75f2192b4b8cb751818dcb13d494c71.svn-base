        <ul class="sidebar-menu">
            <li class="header">MAIN NAVIGATION</li>
            <li class="{% block item_dashboard %} {% endblock %}">
                <a href="{#% url 'dashboard' %#}">
                    <i class="fa fa-dashboard"></i> <span>Dashboard</span>
                    {#                        <small class="label pull-right bg-yellow">12</small>#}
                </a>
            </li>
            <li class="{% block item_customers %} {% endblock %}">
                <a href="{% url 'customers' %}">
                    <i class="fa fa-users"></i> <span>Clientes</span>
                </a>
            </li>
            <li class="{% block item_proposals %} {% endblock %}{% if model == 'proposal' %} active{% endif %}">
                <a href="{% url 'proposal_list' %}">
                    <i class="fa fa-inbox"></i> <span>Presupuestos</span>
                    {#                        <small class="label pull-right bg-yellow">12</small>#}
                </a>
            </li>


            <li class=" {% block item_invoices %} {% endblock %}{% if model == 'invoice' %} active{% endif %}">
                <a href=" {% url 'invoice_list' %}">
                    <i class="fa fa-book"></i> <span>Facturas</span>
                    {#                        <small class="label pull-right bg-yellow">12</small>#}
                </a>
            </li>

            <li class="treeview {% if WEBSITE_STATUS >= 7%} activo{% endif %}  {% if 'site' in request.build_absolute_uri %} active{% endif %}
">

                <a href="{% if WEBSITE_STATUS != 6 %}{% url 'websiteinit' %} {% endif %}">
                    <i class="fa fa-cloud"></i> <span>Sitio web </span> {% if WEBSITE_STATUS == 7 %} <span class="label label-info pull-right"> CREADO</span>{% endif %}
                    {% if WEBSITE_STATUS == 8 %} <span class="label label-success pull-right"> PUBLICADO </span>{% endif %}

                    <small class="label pull-right bg-yellow"></small>
                </a>

                <ul class="{% if WEBSITE_STATUS %}treeview-menu  {% else %} hidden{% endif %} ">
                    <li><a href="{% url 'template' %}" class="{% if 'template' in request.build_absolute_uri and WEBSITE_STATUS %} text-success {% endif %}">Seleccionar plantilla</a></li>
                    <li><a href="{% url 'basic_info' %}" class="{% if 'basic_info' in request.build_absolute_uri and WEBSITE_STATUS  %} text-success {% endif %}" >Informacion básica</a></li>
                    <li><a href="{% url 'gallery' %}" class="{% if 'gallery' in request.build_absolute_uri and WEBSITE_STATUS  %} text-success {% endif %}">Galería de imágenes</a></li>
                    <li><a href="{% url 'menu' %}" class="{% if 'menu' in request.build_absolute_uri and WEBSITE_STATUS  %} text-success {% endif %}">Menu</a></li>
                    <li><a href="{% url 'links' %}" class="{% if 'links' in request.build_absolute_uri and WEBSITE_STATUS  %} text-success {% endif %}">Redes sociales</a></li>
                    <li><a href="{% url 'contactus' %}" class="{% if 'contactus' in request.build_absolute_uri and WEBSITE_STATUS  %} text-success {% endif %}">ContactUs</a></li>
                    <li><a href="{% url 'staffandservice' %}" class="{% if 'staffandservice' in request.build_absolute_uri and WEBSITE_STATUS  %} text-success {% endif %}">Staff and services</a></li>
                    <li><a href="{% url 'congrats' %}" class="{% if 'congrats' in request.build_absolute_uri and WEBSITE_STATUS  %} text-success {% endif %}">Vista previa</a></li>
                </ul>
            </li>
        </ul>
    </section>
    <!-- /.sidebar -->
</aside>


<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">

    {% if EXPIRED %}

        <div class="messages">
            <div class="alert alert-danger ">
                {#                    alert-dismissable#}
                {#                    <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>#}
                <h4>No se ha suscrito o la suscripción ha expirado</h4>

                <p> Algunas funciones se han deshabilitado, click <a
                        href="{% url 'stripe_subscribe' %}">aquí</a> para restablecerlas. </p>
            </div>

        </div>
    {% endif %}
    <!-- Content Header (Page header) -->
    <div {% block style %}  {% endblock %} >
        <section class="content-header">
            {% block content_header %}{% endblock %}
        </section>

        <!-- Main content -->
        <section class="content">
            {% if messages %}
                <div class="django-messages collapse">
                    {% for message in messages %}

                        <div class="alert {% if message.tags %}{{ message.tags }} {% endif %} alert-dismissable">
                            <button type="button" class="close" data-dismiss="alert" aria-hidden="true">×</button>
                            {% if message.tags %} <h4> {{ message.tags |capfirst }}    </h4>{% endif %}
                            <p class="alert-message">{{ message }}</p>
                        </div>

                    {% endfor %}
                </div>
            {% endif %}


            {% get_language_info_list for available_languages as langs %}

            {% for lang in langs %} {{ lang.code }} {% endfor %}
            {% block main_content %}{% endblock %}
        </section>
    </div>


</div>
<!-- /.content-wrapper -->
<footer class="main-footer">
    <div class="pull-right hidden-xs">
        <b>MVP</b> Version
    </div>
    <strong>Copyright &copy; 2016 <a href="http://www.caterfull.com">Caterfull</a>.</strong> All rights reserved.
</footer>


</div>


