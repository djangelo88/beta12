<div class="well white m-t-30">
    <form class="form-floating" ng-submit="saveSocialNetworks(website.links)">
        <fieldset>
            <legend class="subtitle">Social network</legend>

            <div class="form-group filled col-md-8 no-padding-left">
                <label class="control-label">Facebook</label>
                <div class="input-group  ">
                    <input type="text" class="form-control" placeholder=""
                           ng-model="website.links.facebookpage">
                    <div class="input-group-addon">
                        <a href="http://www.facebook.com/" target="_blank"><i class="fa fa-facebook"></i></a>
                    </div>
                </div>
            </div>

            <div class="form-group filled col-md-8 no-padding-left">
                <label class="control-label">Twitter</label>
                <div class="input-group ">
                    <input type="text" class="form-control" placeholder="" ng-model="website.links.twitter">
                    <div class="input-group-addon">
                        <a href="http://www.twitter.com/" target="_blank"><i class="fa fa-twitter"></i></a>
                    </div>
                </div>
            </div>
            <div class="form-group filled col-md-8 no-padding-left">
                <label class="control-label">Instagram</label>
                <div class="input-group filled clearfix">
                    <input type="text" class="form-control" placeholder="" ng-model="website.links.instagram">

                    <div class="input-group-addon">
                        <a href="http://www.instagram.com/" target="_blank"><i class="fa fa-instagram"></i></a>
                    </div>
                </div>
            </div>
            <div class="form-group filled col-md-8 no-padding-left">
                <label class="control-label">Yelp</label>
                <div class="input-group filled clearfix">
                    <input type="text" class="form-control" placeholder="" ng-model="website.links.yelp">
                    <div class="input-group-addon">
                        <a href="http://www.yelp.com/" target="_blank"><i class="fa fa-yelp"></i></a>
                    </div>
                </div>
            </div>
            <div class="form-group filled col-md-8 no-padding-left">
                <label class="control-label">G+</label>
                <div class="input-group filled clearfix">
                    <input type="text" class="form-control" placeholder="" ng-model="website.links.gplus">
                    <div class="input-group-addon">
                        <a href="http://www.gplus.com/" target="_blank"><i class="fa fa-google-plus-official"></i></a>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="form-group text-center">
                <button type="submit" wz-previous class="btn btn-default">Prev</button>
                <button type="submit" class="btn btn-primary" ng-click="onlySave()">Save</button>
                <button type="submit" wz-next class="btn btn-primary" ng-click="saveAndContinue()" >Save and continue</button>
            </div>
        </fieldset>
    </form>
</div>