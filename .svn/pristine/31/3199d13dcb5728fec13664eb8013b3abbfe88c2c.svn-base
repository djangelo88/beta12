<div class="well white m-t-30">
    <form class="form-floating  " ng-submit="saveContactUs(website.contactus)" novalidate="novalidate">
        <fieldset>
            <legend class="subtitle">Contact Us</legend>
            <div class="form-group filled col-md-7 no-padding-left">
                <label class="control-label">Email</label>
                <input type="email" class="form-control" ng-model="website.contactus.email">
            </div>
            <div class="form-group filled col-md-7 no-padding-left">
                <label class="control-label">Phone</label>
                <input type="tel" class="form-control" ng-model="website.contactus.phone">
            </div>
            <div class="form-group filled col-md-7 no-padding-left">
                <label for="textArea" class="control-label ">Address</label>
                        <textarea class="form-control vertical " rows="3" id="textArea"
                                  ng-model="website.contactus.address"></textarea>

            </div>
            <div class="clearfix"></div>
            <div class="form-group text-center">
                <button type="submit" wz-previous class="btn btn-default">Prev</button>
                <button type="submit" class="btn btn-primary" ng-click="onlySave()">Save</button>
                <button type="submit" class="btn btn-primary" ng-click="saveAndContinue()">Save and continue
                </button>
            </div>
        </fieldset>
    </form>
</div>