
.no-padding {
    padding: 0 !important;
}

.no-padding-left {
    padding-left: 0 !important;
}

.no-padding-right {
    padding-right: 0 !important;
}

@media only screen and (max-width: 768px) {
    .no-padding-left-responsive {
        padding-left: 15px;
    }

    .no-padding-right-responsive {
        padding-right: 15px;
    }
}

@media only screen and (min-width: 992px) {
    .no-padding-left-responsive {
        padding-left: 0 !important;
    }

    .no-padding-right-responsive {
        padding-right: 0px !important;
    }
}

.ui-select-focusser {
    width: 100%;
}

.caterfull-form .help-block {
    margin-bottom: 0px !important;
}

.correct-margin-left {
    margin-left: 15px !important;
}

.required-field {
    color: #FF2D2D;
}

.wizard-content {
    margin-top: 30px;
}

.has-error .form-control, .has-error input:not([type=checkbox]) {
    -webkit-box-shadow: inset 0 -1px 0 #F44336 !important;
    box-shadow: inset 0 -1px 0 #F44336 !important;
}

legend.subtitle {
    color: gray;
    font-size: 12pt;
    font-family: Roboto;
    margin-bottom: 0px !important;
}

#proposal_client {
    /*margin-top: 6px;*/
    margin-top: 30px;
}

#proposal_client input.ui-select-focusser {
    height: 1px;
}

#proposal_client #ui-select-choices-0 {
    padding-left: 4px;
}

#proposal_client .has-error.error-msg {
    margin-bottom: 0px;
    margin-top: -15px;
    font-size: 13px;
}

#proposal_client.disabled .select2-container.select2-container-disabled .select2-choice {
    background-color: transparent;
    border: none;
}

.item-preview {

    text-align: right;
    color: #969696;
    font-family: "Roboto", "Helvetica Neue", Helvetica, Arial, sans-serif;
}

.item-preview h4 {
    color: #878787;
    margin-bottom: 0px;
    margin-top: 12px;
    font-size: 11pt;
    color: gray;
}

.items-acction-buttons {
    clear: both;
    float: right;
}

.resend {
    color: #ffab40 !important;
}

.wizard-action-buttons {
    position: fixed !important;
    bottom: 15px;
    width: 33%;
}

td.description-wrap {
    word-wrap: break-word;
    max-width: 140px;
}

.icon-color.icon-color-small {
    width: 28px;
    height: 28px;
    font-size: 17px;
    line-height: 30px;
}

.text-validation-error {
    color: #CC0000;
}