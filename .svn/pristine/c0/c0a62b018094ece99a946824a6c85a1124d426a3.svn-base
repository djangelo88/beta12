/*Proposal/Invoice Detail */
.event-detail {
    font-family: Roboto-Regular;

}

.h1-title {
    background-color: #393939;
    color: #fff;
    text-align: right;
    padding: 5px 15px;
    text-transform: uppercase;
    font-family: RobotoCondensed-Bold;
    letter-spacing: 4pt;
    margin: 0;
}

.h3-title {
    text-transform: uppercase;
    font-family: Roboto-Regular;
    color: #666666;
    margin: 0;
    padding-top: 15px;

}

.client {
    margin-right: 0px;
    margin-bottom: 5px;
}

.event-detail.invoic .client {
    background-color: #D02929;
}

.event-detail.proposal .client {
    background-color: #036E9E;
}

.client p {
    text-transform: uppercase;
    color: #ffffff;
    font-size: 12pt;
    padding: 10px 0;
    margin: 0;
}

.client p:last-child {
    letter-spacing: 1.1pt;
    margin-right: 5px;
}

.descriptions {
    padding-right: 15px;
}

.descriptions p {
    margin-bottom: 0;
}

.event-detail .items {
    margin-top: 30px;
}

.detail-items-table {
    font-size: 10pt;
}

.detail-items-table thead {
    background-color: #393939;
}

.detail-items-table thead tr {
    border: 1px solid #393939;
}

.detail-items-table thead tr th {
    border: 1px solid #393939;
    text-align: center;
    color: #ffffff;
    text-transform: uppercase;
    letter-spacing: 1.1pt;
}

.detail-items-table > tbody > tr > td {
    padding: 4px;
}

.invoic th.descuento {
    border-right: 4px solid #D02929 !important;
}

.proposal th.descuento {
    border-right: 4px solid #036E9E !important;
}

.event-detail.proposal .detail-items-table th.total {
    border: 1px solid #036E9E;
    border-top: 10px solid #036E9E !important;
    background-color: #036E9E;
    border-right: 3px solid #036E9E;
}

.event-detail.proposal .detail-items-table tbody td:last-child {
    border-left: 4px solid #036E9E;
    border-right: 4px solid #036E9E;
}

.event-detail.invoic .detail-items-table th.total {
    border: 1px solid #D02929;
    border-top: 10px solid #D02929 !important;
    background-color: #D02929;
    border-right: 3px solid #D02929;
}

.event-detail.invoic .detail-items-table tbody td:last-child {
    border-left: 4px solid #D02929;
    border-right: 4px solid #D02929;
}

.detail-items-table tfoot tr {
    border: none;
}

.no-border-subtotal {
    border-left: 1px solid transparent !important;
    border-bottom: 1px solid transparent !important;
}

.subtotal-title, .subtotal-value {
    text-transform: uppercase;
    color: #ffffff;
    background-color: #393939;
    padding: 10px !important;
    font-size: 12pt;
}

.subtotal-title {
    padding-left: 5px;
    background-color: #fff;
    color: #666
}

.invoic .subtotal-value {
    text-align: right;
    border-bottom: 6px solid #D02929 !important;
}

.proposal .subtotal-value {
    text-align: right;
    border-bottom: 6px solid #036E9E !important;
}

table .label, li > a .label, .time-label {
    font-family: Helvetica;
}

.event-status {
    font-family: Due;
    font-size: 34pt;
    float: left;
    line-height: 14px;
}

.event-status.denegado, .event-status.deuda {
    color: #dd4b39;
}

.event-status.aceptado {
    color: #8CC63E;
}

.event-status.edicion {
    color: #00c0ef;
}

.event-status.pendiente {
    color: #f39c12;
}

.page-login, .page-error {
    background: radial-gradient(circle at 1.99% 43.04%, rgb(124, 179, 66), transparent 100%), radial-gradient(circle at 99.79% 73.72%, rgba(124, 179, 66, 0.99), transparent 58%), radial-gradient(circle at 50% 50%, rgba(139, 195, 74, 0), rgba(139, 195, 74, 1) 100%) !important;
}

.select2-container.select2-container-disabled .select2-choice {
    background-color: #fff;
    background-image: none;
    border: 1px solid #ddd;
    cursor: default;
}

.fc-state-default {
    background-color: #ffffff;
    background-image: none;
    background-repeat: repeat-x;
    box-shadow: none;
    color: inherit;
    text-shadow: none
}

.fc-state-default {
    border: 1px solid
}

.fc-button {
    color: inherit;
    border: 1px solid #e4e5e7;
    cursor: pointer;
    display: inline-block;
    height: 1.9em;
    line-height: 1.9em;
    overflow: hidden;
    padding: 0 0.6em;
    position: relative;
    white-space: nowrap
}

.fc-state-active {
    background-color: #34495e;
    border-color: #34495e;
    color: #ffffff
}

.fc-header-title h2 {
    font-size: 16px;
    font-weight: 600;
    color: inherit
}

.fc-content .fc-widget-header, .fc-content .fc-widget-content {
    border-color: #e4e5e7;
    font-weight: normal
}

.fc-border-separate tbody {
    background-color: #F8F8F8
}

.fc-state-highlight {
    background: none repeat scroll 0 0 #edf0f5
}

.external-event {
    padding: 5px 10px;
    border-radius: 2px;
    cursor: pointer;
    margin-bottom: 5px
}

.fc-event {
    font-size: .98em !important;
    color: #fff !important;
    border: none !important;
    padding: 2px !important;
    word-wrap: break-word;
}

.fc-ltr .fc-event-hori.fc-event-end, .fc-rtl .fc-event-hori.fc-event-start {
    border-radius: 2px
}

.fc-event, .fc-agenda .fc-event-time, .fc-event a {
    padding: 4px 6px;
    background-color: #34495e;
    border-color: #34495e
}

.fc-event-time, .fc-event-title {
    color: #717171;
    padding: 0 1px
}

.ui-calendar .fc-event-time, .ui-calendar .fc-event-title {
    color: #fff
}

.fc-view-container .fc-widget-header {
    background: #f4f5f9
}

.fc-view-container .fc-widget-content {
    background: #fff
}

.fc-widget-header .fc-day-header {
    padding: 12px 0
}

.fc-ltr .fc-basic-view .fc-day-number {
    text-align: center
}

.fc-basic-view td.fc-day-number, .fc-basic-view td.fc-week-number span {
    padding: 6px
}

.fc-day-grid-event {
    margin: 2px 6px 0
}

.fc-toolbar .fc-center {
    font-family: Roboto-Light;
}

