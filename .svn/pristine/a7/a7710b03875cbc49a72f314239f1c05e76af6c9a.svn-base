
.no-padding {
    padding: 0 !important;
}

.no-padding-left {
    padding-left: 0 !important;
}

.no-padding-right {
    padding-right: 0 !important;
}

@media only screen and (max-width: 768px) {
    .no-padding-left-responsive {
        padding-left: 15px;
    }

    .no-padding-right-responsive {
        padding-right: 15px;
    }
}

@media only screen and (min-width: 992px) {
    .no-padding-left-responsive {
        padding-left: 0 !important;
    }

    .no-padding-right-responsive {
        padding-right: 0px !important;
    }
}

.ui-select-focusser {
    width: 100%;
}

.caterfull-form .help-block {
    margin-bottom: 0px !important;
}

.correct-margin-left {
    margin-left: 15px !important;
}

.required-field {
    color: #FF2D2D;
}

.wizard-content {
    margin-top: 30px;
}

.has-error .form-control, .has-error input:not([type=checkbox]) {
    -webkit-box-shadow: inset 0 -1px 0 #F44336 !important;
    box-shadow: inset 0 -1px 0 #F44336 !important;
}

legend.subtitle {
    color: gray;
    font-size: 12pt;
    font-family: Roboto;
    margin-bottom: 0px !important;
}

#proposal_client {
    /*margin-top: 6px;*/
    margin-top: 30px;
}

#proposal_client input.ui-select-focusser {
    height: 1px;
}

#proposal_client #ui-select-choices-0 {
    padding-left: 4px;
}

#proposal_client .has-error.error-msg {
    margin-bottom: 0px;
    margin-top: -15px;
    font-size: 13px;
}

#proposal_client.disabled .select2-container.select2-container-disabled .select2-choice {
    background-color: transparent;
    border: none;
}

.item-preview {

    text-align: right;
    color: #969696;
    font-family: "Roboto", "Helvetica Neue", Helvetica, Arial, sans-serif;
}

.item-preview h4 {
    color: #878787;
    margin-bottom: 0px;
    margin-top: 12px;
    font-size: 11pt;
    color: gray;
}

.items-acction-buttons {
    clear: both;
    float: right;
}

.resend {
    color: #ffab40 !important;
}

.wizard-action-buttons {
    /*position: fixed !important;*/
    /*bottom: 15px;*/
    /*width: 33%;*/
}

td.description-wrap {
    word-wrap: break-word;
    max-width: 140px;
}

.icon-color.icon-color-small {
    width: 28px;
    height: 28px;
    font-size: 17px;
    line-height: 30px;
}

.text-validation-error {
    color: #CC0000;
}

/*Proposal/Invoice Detail */
.event-detail {
    font-family: Roboto-Regular;

}

.h1-title {
    background-color: #393939;
    color: #fff;
    text-align: right;
    padding: 5px 15px;
    text-transform: uppercase;
    font-family: RobotoCondensed-Bold;
    letter-spacing: 4pt;
    margin: 0;
}

.h3-title {
    text-transform: uppercase;
    font-family: Roboto-Regular;
    color: #666666;
    margin: 0;
    padding-top: 15px;

}

.client {
    margin-right: 0px;
    margin-bottom: 5px;
}

.event-detail.invoic .client {
    background-color: #D02929;
}

.event-detail.proposal .client {
    background-color: #036E9E;
}

.client p {
    text-transform: uppercase;
    color: #ffffff;
    font-size: 12pt;
    padding: 10px 0;
    margin: 0;
}

.client p:last-child {
    letter-spacing: 1.1pt;
    margin-right: 5px;
}

.descriptions {
    padding-right: 15px;
}

.descriptions p {
    margin-bottom: 0;
}

.event-detail .items {
    margin-top: 30px;
}

.detail-items-table {
    font-size: 10pt;
}

.detail-items-table thead {
    background-color: #393939;
}

.detail-items-table thead tr {
    border: 1px solid #393939;
}

.detail-items-table thead tr th {
    border: 1px solid #393939;
    text-align: center;
    color: #ffffff;
    text-transform: uppercase;
    letter-spacing: 1.1pt;
}

.detail-items-table > tbody > tr > td {
    padding: 4px;
}

.invoic th.descuento {
    border-right: 4px solid #D02929 !important;
}

.proposal th.descuento {
    border-right: 4px solid #036E9E !important;
}

.event-detail.proposal .detail-items-table th.total {
    border: 1px solid #036E9E;
    border-top: 10px solid #036E9E !important;
    background-color: #036E9E;
    border-right: 3px solid #036E9E;
}

.event-detail.proposal .detail-items-table tbody td:last-child {
    border-left: 4px solid #036E9E;
    border-right: 4px solid #036E9E;
}

.event-detail.invoic .detail-items-table th.total {
    border: 1px solid #D02929;
    border-top: 10px solid #D02929 !important;
    background-color: #D02929;
    border-right: 3px solid #D02929;
}

.event-detail.invoic .detail-items-table tbody td:last-child {
    border-left: 4px solid #D02929;
    border-right: 4px solid #D02929;
}

.detail-items-table tfoot tr {
    border: none;
}

.no-border-subtotal {
    border-left: 1px solid transparent !important;
    border-bottom: 1px solid transparent !important;
}

.subtotal-title, .subtotal-value {
    text-transform: uppercase;
    color: #ffffff;
    background-color: #393939;
    padding: 10px !important;
    font-size: 12pt;
}

.subtotal-title {
    padding-left: 5px;
    background-color: #fff;
    color: #666
}

.invoic .subtotal-value {
    text-align: right;
    border-bottom: 6px solid #D02929 !important;
}

.proposal .subtotal-value {
    text-align: right;
    border-bottom: 6px solid #036E9E !important;
}

table .label, li > a .label,.time-label{
    font-family: Helvetica;
}

.event-status {
    font-family: Due;
    font-size: 34pt;
    float: left;
    line-height: 14px;
}

.event-status.denegado, .event-status.deuda {
    color: #dd4b39;
}

.event-status.aceptado {
    color: #8CC63E;
}

.event-status.edicion {
    color: #00c0ef;
}
.event-status.pendiente {
    color: #f39c12 ;
}